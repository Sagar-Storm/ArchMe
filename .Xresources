! URxvt Config
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false
URxvt*transparent:       false
URxvt.allow_bold:           true
URxvt.font:                 xft:Pragmata Pro:size=11:antialias=true:autohint=false
URxvt.boldFont:             xft:Pragmata Pro:style=bold:size=12:antialias=true:autohint=false
URxvt.internalBorder: 25
URxvt.letterSpace: 0
URxvt.lineSpace: 4
URxvt.pointerBlank:         true
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false
*antialias:  true
*autohint:   true
URxvt.perl-ext-common: default,resize-font,matcher
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.url-launcher: chromium
URxvt.matcher.button: 1


! Font rendering
Xft.autohint: false
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintsfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb


!rofi Config
rofi.font: Iosevka Regular 12
